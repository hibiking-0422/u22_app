<h1>ユーザー一覧</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>名前</th>
      <th>年齢</th>
    </tr>
  </thead>

  <tbody>
    <% @scores.each do |score| %>
      <tr>
        <td><%= score.user_id %></td>
        <td><%= score.answer %></td>
        <td><%= score.field %></td>
        <td><%= score.fin_field %></td>
        <td><%= score.question %></td>
        <td><%= score.study_day %></td>
      </tr>
    <% end %>

  <%=@tec_total%>
  <%=@stra_total%>
  <%=@mana_total%>


  </tbody>
</table>

